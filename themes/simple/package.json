{
  "name": "@peacock0803sz/theme-simple",
  "version": "0.0.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.49.24",
    "qrcode": "^1.5.4",
    "shiki": "^1.12.1"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.24",
    "@types/qrcode": "^1.5.4"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "light",
    "highlighter": "shiki",
    "defaults": {
      "presenter": "dev",
      "fonts": {
        "sans": "Nunito Sans,BIZ UDPGothic",
        "mono": "JetBrains Mono, monospace",
        "weights": "400,600,700"
      }
    }
  }
}
