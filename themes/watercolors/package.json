  "version": "1.0.0",
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.2"
  },
  "publishConfig": {
    "@peacock0803sz:registry": "https://npm.pkg.github.com"
  },
  "slidev": {
    "colorSchema": "light",
    "highlighter": "shiki",
