{
  "name": "@peacock0803sz/theme-geometricals",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.2"
  },
  "scripts": {
    "dev": "slidev example.md",
    "format": "slidev format"
  },
  "publishConfig": {
    "@peacock0803sz:registry": "https://npm.pkg.github.com"
  },
  "slidev": {
    "colorSchema": "light",
    "highlighter": "shiki",
    "defaults": {
      "aspectRatio": "16/9",
      "canvasWidth": 960,
      "presenter": "dev",
      "fonts": {
        "mono": "JetBrains Mono",
        "sans": "Avenir Next,Murecho,IBM Plex Sans JP",
        "local": "Avenir Next"
      }
    }
  },
  "dependencies": {
    "@slidev/types": "^0.49.26",
    "shiki": "^1.12.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1"
  }
}
